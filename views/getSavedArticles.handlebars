<h1>SAVED ARTICLES</h1>


{{#each savedArticles}}
<div id="{{this._id}}">
    <h3>{{this.title}}</h3>
    <h4><a href="{{this.link}}">link</a></h4>
    <p>{{this.summary}}</p>
    <button id="add-note-btn">ADD NOTE</button>
    <button id="delete-article-btn">DELETE ARTICLE</button>
</div>
{{/each}}

<script src="/js/notes.js" type="text/javascript"></script>